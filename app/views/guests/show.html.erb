<h1>This Guest</h1>

<h3>Guest Name</h3>
<%=@guest.name%>
<br/>
<h3>Guest Occupation</h3>
<%=@guest.occupation%>
<br/>
<h4>List of Episodes w/ Rating</h4>
<%@guest.episodes.each do |episode|%>
